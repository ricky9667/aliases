alias f="fvm flutter" # With FVM (https://github.com/fluttertools/fvm)
# alias f="flutter" # With original Flutter SDK 
alias fd="f doctor"
alias fu="f upgrade"
alias fa="f analyze"
alias fpg="f pub get"
alias fpa="f pub add"
alias fbr="f pub run build_runner build"
alias fbaos="f build appbundle --split-debug-info=build/app/outputs/symbols --obfuscate"
alias fbios="f build ipa --split-debug-info=build/app/outputs/symbols --obfuscate"

